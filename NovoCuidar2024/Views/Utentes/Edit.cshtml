                        <span asp-validation-for="Nome" class="text-danger"></span>
                    </div>
                    <div class="col-md-6">
                        <label asp-for="DataInscricao" class="control-label">Data de Inscrição *</label>
                        <input asp-for="DataInscricao" class="form-control" type="date" />
                        <span asp-validation-for="DataInscricao" class="text-danger"></span>
                    </div>
                </div>
            </div>

            <div class="container">
