                            <option value="">Selecione uma opção</option>
                            @foreach (var item in ViewBag.DataResponsavel)
                            {
                                <option value="@item.Id" selected=SelectedResponsavel>@item.Nome</option>
                            }
                        </select>
                    </div>
                    <div class="col-md-6">
                        <label asp-for="FamiliaId" class="control-label">Identificador de Família</label>
                        <br />
                        <select id="familiaId" name="familiaId" onchange="dropdownFamiliaChanged()">
                            <option value="">Selecione uma opção</option>
                            @foreach (var item in ViewBag.DataFamilia)
                            {
                                <option value="@item.Id" selected=SelectedFamilia>@item.Codigo</option>
                            }
                        </select>

